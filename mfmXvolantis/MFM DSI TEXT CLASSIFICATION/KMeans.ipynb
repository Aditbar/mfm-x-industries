   "source": [
    "import numpy as np\n",
    "from sklearn.cluster import KMeans\n",
    "\n",
    "x = np.array([[1],[2],[3],[6],[7],[8]])\n",
    "km = KMeans(n_clusters=2).fit(x)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Centroid      : [[2.]\n",
      " [7.]]\n",
      "Kelompok Data : [0 0 0 1 1 1]\n"
     ]
    }
   ],
   "source": [
    "print (\"Centroid      : %s\" %km.cluster_centers_) \n",
    "print (\"Kelompok Data : %s\" %km.labels_)"
   ]
  },
